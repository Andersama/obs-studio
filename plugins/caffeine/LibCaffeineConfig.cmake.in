# - Config file for the LibObs package
# It defines the following variables
#  LIBCAFFEINE_INCLUDE_DIRS
#  LIBCAFFEINE_LIBRARIES
#  LIBCAFFEINE_PLUGIN_DESTINATION{,32,64}
#  LIBCAFFEINE_PLUGIN_DATA_DESTINATION

set(LIBCAFFEINE_PLUGIN_DESTINATION "@CONF_PLUGIN_DEST@")
set(LIBCAFFEINE_PLUGIN_DESTINATION32 "@CONF_PLUGIN_DEST32@")
set(LIBCAFFEINE_PLUGIN_DESTINATION64 "@CONF_PLUGIN_DEST64@")
set(LIBCAFFEINE_INCLUDE_DIRS "@CONF_INCLUDE_DIRS@")
set(LIBCAFFEINE_PLUGIN_DATA_DESTINATION "@CONF_PLUGIN_DATA_DEST@")

# Cleanup possible relative paths
get_filename_component(LIBCAFFEINE_PLUGIN_DESTINATION "${LIBCAFFEINE_PLUGIN_DESTINATION}" ABSOLUTE)
get_filename_component(LIBCAFFEINE_PLUGIN_DESTINATION32 "${LIBCAFFEINE_PLUGIN_DESTINATION32}" ABSOLUTE)
get_filename_component(LIBCAFFEINE_PLUGIN_DESTINATION64 "${LIBCAFFEINE_PLUGIN_DESTINATION64}" ABSOLUTE)
get_filename_component(LIBCAFFEINE_INCLUDE_DIRS "${LIBCAFFEINE_INCLUDE_DIRS}" ABSOLUTE)
get_filename_component(LIBCAFFEINE_PLUGIN_DATA_DESTINATION "${LIBCAFFEINE_PLUGIN_DATA_DESTINATION}" ABSOLUTE)

if(NOT TARGET libcaffeine)
	include("${CMAKE_CURRENT_LIST_DIR}/LibCaffeineTarget.cmake")
endif()

set(LIBOBS_LIBRARIES libcaffeine)
